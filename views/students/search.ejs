<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h1>Search Results</h1>
        </div>
    </div>
</div>          

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <% if(student != "") {%>
                 <% student.forEach(function(student, index) { %>
                    <span><%- student.name %></span>
                    <span><%- student.phone_number %></span>
                    <span><%- student.email %></span>
                    <span><%- student.date %></span>
                    <span><%- student.description %></span>
            <%}) %>    
            <% } else { %>
                <p>No people found</p>
            <% } %>
        </div>
    </div>
</div>  